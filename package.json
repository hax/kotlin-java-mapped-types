{
  "name": "kotlin-java-mapped-types",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "This project documents the mapped types between Kotlin and Java as specified in the [Kotlin documentation](https://kotlinlang.org/docs/java-interop.html#mapped-types).",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "start": "node --run gen:defs && node --run gen:mt",
    "get:mt": "node lib/cli/get-mapped-types.ts --offline",
    "get:def": "node lib/cli/get-def.ts --offline",
    "gen:defs": "node lib/cli/gen-defs.ts",
    "calc:mappings": "node lib/cli/calc-mappings.ts",
    "gen:mt": "node lib/cli/gen-mapped-types.ts",
    "convert": "node lib/cli/convert-to-kotlin.ts",
    "map": "node lib/cli/map-to-dts.ts",
    "demo": "node lib/cli/demo-converter.ts",
    "test": "node --run typecheck"
  },
  "keywords": [],
  "author": "hax",
  "devDependencies": {
    "@types/make-fetch-happen": "^10.0.4",
    "@types/node": "^25.0.3",
    "cheerio": "^1.1.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "make-fetch-happen": "^15.0.3"
  }
}
